Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"polyfit", "[", 
    RowBox[{"xtrain_", ",", "ytrain_", ",", "degree_", ",", "typePolynom_"}], 
    "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"polytrain", "=", 
        RowBox[{"typePolynom", "[", 
         RowBox[{"xtrain", ",", "degree"}], "]"}]}], ",", 
       RowBox[{"coefs", "=", 
        RowBox[{"Thread", "[", 
         RowBox[{"coefficients", "[", 
          RowBox[{"Range", "[", 
           RowBox[{"0", ",", "degree"}], "]"}], "]"}], "]"}]}], ",", 
       RowBox[{"l", "=", 
        RowBox[{"Length", "[", "xtrain", "]"}]}]}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"lsm", "=", 
       RowBox[{"Total", "@", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"polytrain", "-", "ytrain"}], ")"}], "2"]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"fitLSM", "=", 
       RowBox[{"Minimize", "[", 
        RowBox[{"lsm", ",", "coefs"}], "]"}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"yFit", "=", 
       RowBox[{"polytrain", "/.", 
        RowBox[{
        "fitLSM", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}]}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"mse", " ", "=", " ", 
       FractionBox[
        RowBox[{"Total", "[", 
         SuperscriptBox[
          RowBox[{"(", 
           RowBox[{"yFit", "-", "ytrain"}], ")"}], "2"], "]"}], "l"]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"pointsYpred", "=", 
       RowBox[{"Thread", "[", 
        RowBox[{"point", "[", 
         RowBox[{"xtrain", ",", "yFit"}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"pointsYpred", ",", "mse", ",", 
