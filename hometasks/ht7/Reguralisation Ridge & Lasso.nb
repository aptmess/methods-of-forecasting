Cell[BoxData[{
 RowBox[{"Clear", "[", 
  RowBox[{"\[Lambda]", ",", "ridgePolynom"}], "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"polyRidge", "[", 
   RowBox[{
   "xtrain_", ",", "ytrain_", ",", "degree_", ",", "typePolynom_", ",", 
    "\[Lambda]_", ",", "step_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"polytrain", "=", 
       RowBox[{"typePolynom", "[", 
        RowBox[{"xtrain", ",", "degree", ",", "\[Lambda]", ",", "step"}], 
        "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"coefs", "=", 
       RowBox[{"Thread", "[", 
        RowBox[{"coefficients", "[", 
         RowBox[{"Range", "[", 
          RowBox[{"0", ",", "degree"}], "]"}], "]"}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"l", "=", 
       RowBox[{"Length", "[", "xtrain", "]"}]}]}], "\[IndentingNewLine]", 
     "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"lsm", "=", 
      RowBox[{"Total", "@", 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"polytrain", "-", "ytrain"}], ")"}], "2"]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"fitLSM", "=", 
      RowBox[{"Minimize", "[", 
       RowBox[{"lsm", ",", "coefs"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"yFit", "=", 
      RowBox[{"polytrain", "/.", 
       RowBox[{
       "fitLSM", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"mse", " ", "=", " ", 
      FractionBox[
       RowBox[{"Total", "[", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"yFit", "-", "ytrain"}], ")"}], "2"], "]"}], "l"]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"pointsYpred", "=", 
      RowBox[{"Thread", "[", 
       RowBox[{"point", "[", 
        RowBox[{"xtrain", ",", "yFit"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"{", 
      RowBox[{"pointsYpred", ",", "mse", ",", 
       RowBox[{
       "fitLSM", "\[LeftDoubleBracket]", "2", "\[RightDoubleBracket]"}]}], 
      "}"}]}]}], "]"}]}], "\[IndentingNewLine]"}], "Input",
